{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "vercel-sapper"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "main.ts",
        "methods": [
          "GET",
          "POST",
          "PUT",
          "DELETE"
        ]
      }
    ]
  }